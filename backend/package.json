{
  "name": "backend",
  "version": "1.0.0",
  "description": "MERN Backend Server",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "_comment": "  'start': 'NODE_ENV=production node server.js'",
    "start": " cls && node src/server.js",
    "dev": "cls && nodemon src/server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest --detectOpenHandles --forceExit",
    "build": "babel src -d dist"
